<h1 class=title>New post</h1>
<!-- form_with部分-->
<%= form_with model: @list, url:'/todolists', local:true do |f| %>
  <h4 class=title>Title</h4>
  <%= f.text_field :title %>
  <h4 class=title>Text</h4>
  <%= f.text_area :body %>
  <!--追加部分-->
  <h4 class=title>Photo</h4>
  <%= f.attachment_field :image %>

  <%= f.submit '投稿' %>
<% end %>

